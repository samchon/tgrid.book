{
    "title": "TGrid",
    "description": "Guide Documents for TGrid",
    "author": "Jeongho Nam",

    "plugins": [
        "-highlight",
        "addcssjs",
        "back-to-top-button",
        "codegroup",
        "collapse",
        "disqus",
        "github",
        "hide-navigation-buttons",
        "hide-published-with",
        "import",
        "panel",
        "prism",
        "scroll-to-top",
        "theme-darkblue",
        "theme-github",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "codegroup": {
            "rememberTabs": true
        },
        "disqus": {
            "shortName": "tgrid"
        },
        "github": {
            "url": "https://github.com/samchon/tgrid"
        },
        "sitemap-general": {
            "prefix": "https://tgrid.dev/"
        }
    }
}