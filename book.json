{
    "title": "TGrid",
    "description": "Guide Documents for TGrid",
    "author": "Jeongho Nam",

    "plugins": [
        "-highlight",
        "addcssjs",
        "back-to-top-button",
        "codegroup",
        "collapse",
        "disqus",
        "ga",
        "github",
        "hide-navigation-buttons",
        "hide-published-with",
        "import",
        "language-picker",
        "meta",
        "panel",
        "prism",
        "scroll-to-top",
        "theme-darkblue",
        "theme-github",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "codegroup": {
            "rememberTabs": true
        },
        "disqus": {
            "shortName": "tgrid"
        },
        "ga": {
            "token": "UA-146254534-1"
        },
        "github": {
            "url": "https://github.com/samchon/tgrid"
        },
        "meta": {
            "data": [
                { "name": "og:url", "content": "https://tgrid.dev" },
                { "name": "og:type", "content": "website" },
                { "name": "og:title", "content": "TGrid Guide Documents" },
                { "name": "og:description", "content": "TypeScript Grid Computing Framework" },
                { "name": "og:image", "content": "https://tgrid.dev/assets/images/flag.png" }
            ]
        },
        "sitemap-general": {
            "prefix": "https://tgrid.dev/"
        }
    }
}