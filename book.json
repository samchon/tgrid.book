{
    "title": "TGrid",
    "description": "Guide Documents for TGrid",
    "author": "Jeongho Nam",

    "output": "./docs",
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },

    "plugins": [
        "-highlight",
        "addcssjs",
        "code",
        "codegroup",
        "disqus",
        "github",
        "hide-navigation-buttons",
        "hide-published-with",
        "panel",
        "prism",
        "theme-darkblue",
        "theme-github",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "codegroup": {
            "rememberTabs": true
        },
        "disqus": {
            "shortName": "tgrid"
        },
        "github": {
            "url": "https://github.com/samchon/tgrid"
        },
        "sitemap-general": {
            "prefix": "https://tgrid.dev/"
        }
    }
}